# Add build compilation ut sub-directories according to the configured modules:
#
# obj-$(CONFIG_<MODULE>) += <module>

# sliding window
obj-$(CONFIG_LIB_SLW)		+= slw

# device tree
obj-$(CONFIG_DEV_TREE)		+= dev_tree

# mmc debugfs 
obj-$(CONFIG_DEBUG_FS)		+= mmc_debugfs

# ffu
obj-$(CONFIG_MMC_FFU)		+= ffu
